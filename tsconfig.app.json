{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./static/build/out-tsc",
    "types": [
      "node",

      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ]
  },
  "exclude": [
    "jest.config.ts",
    "src/app/webui/**/*.spec.ts",
    "src/app/webui/**/*.test.ts",
    "src/app/webui/**/*.spec.tsx",
    "src/app/webui/**/*.test.tsx",
    "src/app/webui/**/*.spec.js",
    "src/app/webui/**/*.test.js",
    "src/app/webui/**/*.spec.jsx",
    "src/app/webui/**/*.test.jsx"
  ],
  "include": ["src/app/webui/**/*.js", "src/app/webui/**/*.jsx", "app_frontend/**/*.ts", "src/app/webui/**/*.tsx"]
}
